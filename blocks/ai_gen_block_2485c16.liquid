{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-warranty-content-{{ ai_gen_id }} {
    max-width: {{ block.settings.max_width }}px;
    width: {{ block.settings.content_width }}%;
    margin: 0 auto;
    padding: {{ block.settings.section_padding }}px;
    background-color: {{ block.settings.background_color }};
    color: {{ block.settings.text_color }};
    font-family: {{ block.settings.body_font.family }}, {{ block.settings.body_font.fallback_families }};
    font-size: {{ block.settings.body_font_size }}px;
    line-height: {{ block.settings.line_height }};
  }

  .ai-warranty-content-{{ ai_gen_id }} .ai-warranty-title-{{ ai_gen_id }} {
    font-family: {{ block.settings.heading_font.family }}, {{ block.settings.heading_font.fallback_families }};
    font-size: {{ block.settings.title_font_size }}px;
    color: {{ block.settings.title_color }};
    margin: 0 0 {{ block.settings.title_spacing }}px 0;
    font-weight: {{ block.settings.title_font_weight }};text-align: {{ block.settings.title_alignment }};
  }

  .ai-warranty-content-{{ ai_gen_id }} .ai-warranty-subtitle-{{ ai_gen_id }} {
    font-family: {{ block.settings.heading_font.family }}, {{ block.settings.heading_font.fallback_families }};
    font-size: {{ block.settings.subtitle_font_size }}px;
    color: {{ block.settings.subtitle_color }};
    margin: {{ block.settings.section_title_spacing }}px 0 {{ block.settings.subtitle_spacing }}px 0;
    font-weight: {{ block.settings.subtitle_font_weight }};
  }

  .ai-warranty-content-{{ ai_gen_id }} .ai-warranty-paragraph-{{ ai_gen_id }} {
    margin: 0 0 {{ block.settings.paragraph_spacing }}px 0;
  }

  .ai-warranty-content-{{ ai_gen_id }} .ai-warranty-email-{{ ai_gen_id }} {
    color: {{ block.settings.link_color }};
    text-decoration: underline;
  }

  .ai-warranty-content-{{ ai_gen_id }} .ai-warranty-email-{{ ai_gen_id }}:hover {
    color: {{ block.settings.link_hover_color }};
  }

  .ai-warranty-content-{{ ai_gen_id }} .ai-warranty-warning-{{ ai_gen_id }} {
    background-color: {{ block.settings.warning_bg_color }};
    color: {{ block.settings.warning_text_color }};
    padding: {{ block.settings.warning_padding }}px;
    border-radius: {{ block.settings.warning_border_radius }}px;
    margin: {{ block.settings.warning_spacing }}px 0;
    border-left: 4px solid {{ block.settings.warning_border_color }};
  }

  .ai-warranty-content-{{ ai_gen_id }} .ai-warranty-warning-icon-{{ ai_gen_id }} {
    display: inline-block;
    margin-right: 8px;
    font-weight: bold;
  }

  .ai-warranty-content-{{ ai_gen_id }} .ai-warranty-contact-info-{{ ai_gen_id }} {
    background-color: {{ block.settings.contact_bg_color }};
    padding: {{ block.settings.contact_padding }}px;
    border-radius: {{ block.settings.contact_border_radius }}px;
    margin: {{ block.settings.contact_spacing }}px 0;border: 1px solid {{ block.settings.contact_border_color }};
  }

  .ai-warranty-content-{{ ai_gen_id }} .ai-warranty-contact-item-{{ ai_gen_id }} {
    display: flex;
    align-items: center;
    margin-bottom: {{ block.settings.contact_item_spacing }}px;font-size: {{ block.settings.contact_font_size }}px;
  }

  .ai-warranty-content-{{ ai_gen_id }} .ai-warranty-contact-item-{{ ai_gen_id }}:last-child {
    margin-bottom: 0;
  }

  .ai-warranty-content-{{ ai_gen_id }} .ai-warranty-contact-icon-{{ ai_gen_id }} {
    margin-right: 12px;
    font-size: {{ block.settings.contact_icon_size }}px;width: 24px;
    text-align: center;
  }

  .ai-warranty-content-{{ ai_gen_id }} .ai-warranty-contact-link-{{ ai_gen_id }} {
    color: {{ block.settings.contact_link_color }};
    text-decoration: none;
  }

  .ai-warranty-content-{{ ai_gen_id }} .ai-warranty-contact-link-{{ ai_gen_id }}:hover {
    color: {{ block.settings.contact_link_hover_color }};
    text-decoration: underline;
  }

  @media screen and (max-width:749px) {
    .ai-warranty-content-{{ ai_gen_id }} {
      padding: {{ block.settings.mobile_padding }}px;
      width: {{ block.settings.mobile_content_width }}%;
    }
    .ai-warranty-content-{{ ai_gen_id }} .ai-warranty-title-{{ ai_gen_id }} {
      font-size: {{ block.settings.mobile_title_size }}px;
    }
    
    .ai-warranty-content-{{ ai_gen_id }} .ai-warranty-subtitle-{{ ai_gen_id }} {
      font-size: {{ block.settings.mobile_subtitle_size }}px;
    }

    .ai-warranty-content-{{ ai_gen_id }} .ai-warranty-contact-info-{{ ai_gen_id }} {
      padding: {{ block.settings.mobile_contact_padding }}px;
    }

    .ai-warranty-content-{{ ai_gen_id }} .ai-warranty-contact-item-{{ ai_gen_id }} {
      font-size: {{ block.settings.mobile_contact_font_size }}px;
    }
  }
{% endstyle %}

<div class="ai-warranty-content-{{ ai_gen_id }}" {{ block.shopify_attributes }}>
  {% if block.settings.main_title != blank %}
    <h1 class="ai-warranty-title-{{ ai_gen_id }}">{{ block.settings.main_title }}</h1>
  {% endif %}

  {% if block.settings.intro_text != blank %}
    <div class="ai-warranty-paragraph-{{ ai_gen_id }}">{{ block.settings.intro_text }}</div>
  {% endif %}

  {% if block.settings.show_non_electric_section %}
    <h2 class="ai-warranty-subtitle-{{ ai_gen_id }}">{{ block.settings.non_electric_title }}</h2>
    <div class="ai-warranty-paragraph-{{ ai_gen_id }}">{{ block.settings.non_electric_warranty_text }}</div>
    <div class="ai-warranty-paragraph-{{ ai_gen_id }}">{{ block.settings.non_electric_coverage_text }}</div>
    <div class="ai-warranty-paragraph-{{ ai_gen_id }}">{{ block.settings.non_electric_discretion_text }}</div>
  {% endif %}

  {% if block.settings.show_electric_section %}
    <h2 class="ai-warranty-subtitle-{{ ai_gen_id }}">{{ block.settings.electric_title }}</h2>
    <div class="ai-warranty-paragraph-{{ ai_gen_id }}">{{ block.settings.electric_warranty_text }}</div>
    <div class="ai-warranty-paragraph-{{ ai_gen_id }}">{{ block.settings.electric_coverage_text }}</div>
    <div class="ai-warranty-paragraph-{{ ai_gen_id }}">{{ block.settings.electric_discretion_text }}</div>
    <div class="ai-warranty-paragraph-{{ ai_gen_id }}">{{ block.settings.legal_rights_text }}</div>
    <div class="ai-warranty-paragraph-{{ ai_gen_id }}">{{ block.settings.electric_contact_text }}</div>
  {% endif %}

  {% if block.settings.show_warning %}
    <div class="ai-warranty-warning-{{ ai_gen_id }}">
      <span class="ai-warranty-warning-icon-{{ ai_gen_id }}">‚ö†Ô∏è</span>
      {{ block.settings.warning_text }}
    </div>
  {% endif %}

  {% if block.settings.show_conditions_section %}
    <h2 class="ai-warranty-subtitle-{{ ai_gen_id }}">{{ block.settings.conditions_title }}</h2>
    <div class="ai-warranty-paragraph-{{ ai_gen_id }}">{{ block.settings.conditions_text_1 }}</div>
    <div class="ai-warranty-paragraph-{{ ai_gen_id }}">{{ block.settings.conditions_text_2 }}</div>
    <div class="ai-warranty-paragraph-{{ ai_gen_id }}">{{ block.settings.conditions_text_3 }}</div>
  {% endif %}

  {% if block.settings.show_distributors_section %}
    <h2 class="ai-warranty-subtitle-{{ ai_gen_id }}">{{ block.settings.distributors_title }}</h2>
    <div class="ai-warranty-paragraph-{{ ai_gen_id }}">{{ block.settings.distributors_text }}</div>
  {% endif %}

  {% if block.settings.show_contact_section %}
    <h2 class="ai-warranty-subtitle-{{ ai_gen_id }}">{{ block.settings.contact_title }}</h2>
    {% if block.settings.contact_intro_text != blank %}
      <div class="ai-warranty-paragraph-{{ ai_gen_id }}">{{ block.settings.contact_intro_text }}</div>
    {% endif %}
    <div class="ai-warranty-contact-info-{{ ai_gen_id }}">
      {% if block.settings.contact_email != blank %}
        <div class="ai-warranty-contact-item-{{ ai_gen_id }}">
          <span class="ai-warranty-contact-icon-{{ ai_gen_id }}">üìß</span>
          <span>{{ block.settings.contact_email_label }}: 
            <a href="mailto:{{ block.settings.contact_email }}" class="ai-warranty-contact-link-{{ ai_gen_id }}">{{ block.settings.contact_email }}</a>
          </span>
        </div>
      {% endif %}

      {% if block.settings.contact_call_center != blank %}
        <div class="ai-warranty-contact-item-{{ ai_gen_id }}">
          <span class="ai-warranty-contact-icon-{{ ai_gen_id }}">üìû</span>
          <span>{{ block.settings.contact_call_center_label }}: 
            <a href="tel:{{ block.settings.contact_call_center | replace: ' ', '' | replace: '(', '' | replace: ')', '' | replace: '-', '' }}" class="ai-warranty-contact-link-{{ ai_gen_id }}">{{ block.settings.contact_call_center }}</a>
          </span>
        </div>
      {% endif %}

      {% if block.settings.contact_sales != blank %}
        <div class="ai-warranty-contact-item-{{ ai_gen_id }}">
          <span class="ai-warranty-contact-icon-{{ ai_gen_id }}">üìû</span>
          <span>{{ block.settings.contact_sales_label }}: 
            <a href="tel:{{ block.settings.contact_sales | replace: ' ', '' | replace: '(', '' | replace: ')', '' | replace: '-', '' }}" class="ai-warranty-contact-link-{{ ai_gen_id }}">{{ block.settings.contact_sales }}</a>
          </span>
        </div>
      {% endif %}

      {% if block.settings.contact_website != blank %}
        <div class="ai-warranty-contact-item-{{ ai_gen_id }}">
          <span class="ai-warranty-contact-icon-{{ ai_gen_id }}">üåê</span>
          <span>{{ block.settings.contact_website_label }}: 
            <a href="{% unless block.settings.contact_website contains 'http' %}https://{% endunless %}{{ block.settings.contact_website }}" class="ai-warranty-contact-link-{{ ai_gen_id }}" target="_blank" rel="noopener">{{ block.settings.contact_website }}</a>
          </span>
        </div>
      {% endif %}
    </div>
  {% endif %}
</div>

