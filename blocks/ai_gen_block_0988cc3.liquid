{% doc %}
  @prompt
    Generate a 5 columns  small banners to display the technology behind our helmets.    with a background image an small heading  short sub heading and button.  everything gotta be fully customizable. , just in mobile:  make it slider. , section padding top and bottom are not working , add a foreground logo in each card with position controls.   add some hover effects ( b/w, blur, etc) 
    
    add the option for b7w image and color in hover. 
    
    add 40px margin from top of the section and 40px  bottom. 
    
    add option for background color , remove the round effect from the foreground logo image 
{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-tech-banners-{{ ai_gen_id }} {
    width: 100%;
    padding-top: {{ block.settings.section_padding_top }}px;
    padding-bottom: {{ block.settings.section_padding_bottom }}px;
    margin-top: {{ block.settings.section_margin_top }}px;
    margin-bottom: {{ block.settings.section_margin_bottom }}px;
    background-color: {{ block.settings.section_background_color }};

  }

  .ai-tech-banners__container-{{ ai_gen_id }} {
    max-width: {{ block.settings.container_width }}px;
    margin: 0 auto;

    padding-top: 40px;
    padding-bottom: 0px;
  }

  .ai-tech-banners__grid-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: repeat(5, 1fr);
    gap: {{ block.settings.gap }}px;
  }

  .ai-tech-banner-{{ ai_gen_id }} {
    position: relative;
    overflow: hidden;
    border-radius: {{ block.settings.border_radius }}px;
    height: {{ block.settings.banner_height }}px;
    display: flex;
    flex-direction: column;
    justify-content: flex-end;
    padding: {{ block.settings.content_padding }}px;
    text-align: {{ block.settings.text_alignment }};
    cursor: pointer;
  }

  .ai-tech-banner__image-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    object-fit: cover;
    z-index: 1;
    transition: all 0.4s ease;
  }

  {% if block.settings.hover_effect == 'grayscale' %}
    .ai-tech-banner-{{ ai_gen_id }}:hover .ai-tech-banner__image-{{ ai_gen_id }} {
      filter: grayscale(100%);
    }
  {% elsif block.settings.hover_effect == 'blur' %}
    .ai-tech-banner-{{ ai_gen_id }}:hover .ai-tech-banner__image-{{ ai_gen_id }} {
      filter: blur(4px);
    }
  {% elsif block.settings.hover_effect == 'zoom' %}
    .ai-tech-banner-{{ ai_gen_id }}:hover .ai-tech-banner__image-{{ ai_gen_id }} {
      transform: scale(1.1);
    }
  {% elsif block.settings.hover_effect == 'grayscale_zoom' %}
    .ai-tech-banner-{{ ai_gen_id }}:hover .ai-tech-banner__image-{{ ai_gen_id }} {
      filter: grayscale(100%);
      transform: scale(1.1);
    }
  {% endif %}

  .ai-tech-banner__placeholder-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 1;
    background-color: #f4f4f4;
  }

  .ai-tech-banner__placeholder-{{ ai_gen_id }} svg {
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .ai-tech-banner__overlay-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: {{ block.settings.overlay_color }};
    opacity: {{ block.settings.overlay_opacity | divided_by: 100.0 }};
    z-index: 2;
    transition: all 0.4s ease;
  }

  {% if block.settings.hover_overlay_enabled %}
    .ai-tech-banner-{{ ai_gen_id }}:hover .ai-tech-banner__overlay-{{ ai_gen_id }} {
      background-color: {{ block.settings.hover_overlay_color }};
      opacity: {{ block.settings.hover_overlay_opacity | divided_by: 100.0 }};
    }
  {% endif %}

  .ai-tech-banner__logo-{{ ai_gen_id }} {
    position: absolute;
    z-index: 3;
    width: {{ block.settings.logo_width }}px;
    height: auto;
    transition: all 0.4s ease;
    {% if block.settings.logo_position == 'top_left' %}
      top: {{ block.settings.logo_offset_vertical }}px;
      left: {{ block.settings.logo_offset_horizontal }}px;
    {% elsif block.settings.logo_position == 'top_center' %}
      top: {{ block.settings.logo_offset_vertical }}px;
      left: 50%;
      transform: translateX(-50%);
    {% elsif block.settings.logo_position == 'top_right' %}
      top: {{ block.settings.logo_offset_vertical }}px;
      right: {{ block.settings.logo_offset_horizontal }}px;
    {% elsif block.settings.logo_position == 'center_left' %}
      top: 50%;
      left: {{ block.settings.logo_offset_horizontal }}px;
      transform: translateY(-50%);
    {% elsif block.settings.logo_position == 'center' %}
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%);
    {% elsif block.settings.logo_position == 'center_right' %}
      top: 50%;
      right: {{ block.settings.logo_offset_horizontal }}px;
      transform: translateY(-50%);
    {% elsif block.settings.logo_position == 'bottom_left' %}
      bottom: {{ block.settings.logo_offset_vertical }}px;
      left: {{ block.settings.logo_offset_horizontal }}px;
    {% elsif block.settings.logo_position == 'bottom_center' %}
      bottom: {{ block.settings.logo_offset_vertical }}px;
      left: 50%;
      transform: translateX(-50%);
    {% elsif block.settings.logo_position == 'bottom_right' %}
      bottom: {{ block.settings.logo_offset_vertical }}px;
      right: {{ block.settings.logo_offset_horizontal }}px;
    {% endif %}
  }

  {% if block.settings.logo_hover_effect == 'grayscale' %}
    .ai-tech-banner-{{ ai_gen_id }}:hover .ai-tech-banner__logo-{{ ai_gen_id }} {
      filter: grayscale(100%);
    }
  {% elsif block.settings.logo_hover_effect == 'scale' %}
    .ai-tech-banner-{{ ai_gen_id }}:hover .ai-tech-banner__logo-{{ ai_gen_id }} {
      {% if block.settings.logo_position == 'top_center' %}
        transform: translateX(-50%) scale(1.2);
      {% elsif block.settings.logo_position == 'center_left' or block.settings.logo_position == 'center_right' %}
        transform: translateY(-50%) scale(1.2);
      {% elsif block.settings.logo_position == 'center' %}
        transform: translate(-50%, -50%) scale(1.2);
      {% elsif block.settings.logo_position == 'bottom_center' %}
        transform: translateX(-50%) scale(1.2);
      {% else %}
        transform: scale(1.2);
      {% endif %}
    }
  {% elsif block.settings.logo_hover_effect == 'fade' %}
    .ai-tech-banner-{{ ai_gen_id }}:hover .ai-tech-banner__logo-{{ ai_gen_id }} {
      opacity: 0.5;
    }
  {% endif %}

  .ai-tech-banner__content-{{ ai_gen_id }} {
    position: relative;
    z-index: 3;
  }

  .ai-tech-banner__heading-{{ ai_gen_id }} {
    color: {{ block.settings.heading_color }};
    font-size: {{ block.settings.heading_size }}px;
    font-weight: {{ block.settings.heading_weight }};
    margin: 0 0 {{ block.settings.heading_spacing }}px;
    line-height: 1.2;
  }

  .ai-tech-banner__subheading-{{ ai_gen_id }} {
    color: {{ block.settings.subheading_color }};
    font-size: {{ block.settings.subheading_size }}px;
    margin: 0 0 {{ block.settings.subheading_spacing }}px;
    line-height: 1.4;
  }

  .ai-tech-banner__button-{{ ai_gen_id }} {
    display: inline-block;
    padding: {{ block.settings.button_padding_vertical }}px {{ block.settings.button_padding_horizontal }}px;
    background-color: {{ block.settings.button_bg_color }};
    color: {{ block.settings.button_text_color }};
    text-decoration: none;
    border-radius: {{ block.settings.button_border_radius }}px;
    font-size: {{ block.settings.button_font_size }}px;
    font-weight: {{ block.settings.button_font_weight }};
    transition: all 0.3s ease;
    border: {{ block.settings.button_border_width }}px solid {{ block.settings.button_border_color }};
  }

  .ai-tech-banner__button-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.button_bg_hover_color }};
    color: {{ block.settings.button_text_hover_color }};
    border-color: {{ block.settings.button_border_hover_color }};
  }

  .ai-tech-banners__slider-wrapper-{{ ai_gen_id }} {
    display: none;
  }

  @media screen and (max-width: 1200px) {
    .ai-tech-banners__grid-{{ ai_gen_id }} {
      grid-template-columns: repeat(3, 1fr);
    }
  }

  @media screen and (max-width: 749px) {
    .ai-tech-banners__container-{{ ai_gen_id }} {
      padding: 0;
    }

    .ai-tech-banners__grid-{{ ai_gen_id }} {
      display: none;
    }

    .ai-tech-banners__slider-wrapper-{{ ai_gen_id }} {
      display: block;
      position: relative;
      overflow: hidden;
      padding: 0 20px;
    }

    .ai-tech-banners__slider-{{ ai_gen_id }} {
      display: flex;
      gap: {{ block.settings.gap }}px;
      overflow-x: auto;
      scroll-snap-type: x mandatory;
      scrollbar-width: none;
      -webkit-overflow-scrolling: touch;
      padding-bottom: 10px;
    }

    .ai-tech-banners__slider-{{ ai_gen_id }}::-webkit-scrollbar {
      display: none;
    }

    .ai-tech-banners__slider-{{ ai_gen_id }} .ai-tech-banner-{{ ai_gen_id }} {
      flex: 0 0 85%;
      scroll-snap-align: start;
      height: {{ block.settings.banner_height_mobile }}px;
    }

    .ai-tech-banner__heading-{{ ai_gen_id }} {
      font-size: {{ block.settings.heading_size | times: 0.8 }}px;
    }

    .ai-tech-banner__subheading-{{ ai_gen_id }} {
      font-size: {{ block.settings.subheading_size | times: 0.85 }}px;
    }

    .ai-tech-banner__button-{{ ai_gen_id }} {
      font-size: {{ block.settings.button_font_size | times: 0.9 }}px;
      padding: {{ block.settings.button_padding_vertical | times: 0.8 }}px {{ block.settings.button_padding_horizontal | times: 0.8 }}px;
    }

    .ai-tech-banner__logo-{{ ai_gen_id }} {
      width: {{ block.settings.logo_width | times: 0.8 }}px;
    }

    .ai-tech-banners__dots-{{ ai_gen_id }} {
      display: flex;
      justify-content: center;
      gap: 8px;
      margin-top: 16px;
    }

    .ai-tech-banners__dot-{{ ai_gen_id }} {
      width: 8px;
      height: 8px;
      border-radius: 50%;
      background-color: {{ block.settings.dot_color }};
      opacity: 0.3;
      transition: opacity 0.3s ease;
    }

    .ai-tech-banners__dot-{{ ai_gen_id }}.active {
      opacity: 1;
    }
  }


  /* Force square logo â€” override any global img rules */
.ai-tech-banner__logo-{{ ai_gen_id }} {
  position: absolute;
  z-index: 3;
  width: {{ block.settings.logo_width }}px;
  height: auto;
  transition: all 0.4s ease;

  /* ----- the important lines ----- */
  border-radius: 0 !important;
  -webkit-border-radius: 0 !important;
  clip-path: none !important;
  overflow: visible !important;
  object-fit: contain;
  background: transparent !important;
  box-shadow: none !important;
}


@media screen and (max-width: 749px) {
  /* Mobile: 40px top padding only */
  .ai-tech-banners__container-{{ ai_gen_id }} {
    padding-top: 40px;
    padding-right: 0;
    padding-left: 0;
    padding-bottom: 0px;
  }

  /* keep slider wrapper horizontal padding as-is, but if you want full-bleed remove side padding */
  .ai-tech-banners__slider-wrapper-{{ ai_gen_id }} {
    padding: 0 20px; /* deja esto si quieres margen lateral para el slider en mobile */
  }
}

{% endstyle %}

<tech-banners-slider-{{ ai_gen_id }} class="ai-tech-banners-{{ ai_gen_id }}" {{ block.shopify_attributes }}>
