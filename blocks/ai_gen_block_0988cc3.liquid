          {% liquid
            assign banner_enabled_key = 'banner_' | append: i | append: '_enabled'
            assign banner_image_key = 'banner_' | append: i | append: '_image'
            assign banner_logo_key = 'banner_' | append: i | append: '_logo'
            assign banner_heading_key = 'banner_' | append: i | append: '_heading'
            assign banner_subheading_key = 'banner_' | append: i | append: '_subheading'
            assign banner_button_text_key = 'banner_' | append: i | append: '_button_text'
            assign banner_button_link_key = 'banner_' | append: i | append: '_button_link'

            assign banner_enabled = block.settings[banner_enabled_key]
            assign banner_image = block.settings[banner_image_key]
            assign banner_logo = block.settings[banner_logo_key]
            assign banner_heading = block.settings[banner_heading_key]
            assign banner_subheading = block.settings[banner_subheading_key]
