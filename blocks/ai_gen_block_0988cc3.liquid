</tech-banners-slider-{{ ai_gen_id }}>

<script>
  (function() {
    class TechBannersSlider{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
        this.slider = null;
        this.dotsContainer = null;
        this.slides = [];
        this.currentIndex = 0;
      }

      connectedCallback() {
        if (window.innerWidth <= 749) {
          this.init();
        }

        window.addEventListener('resize', () => {
          if (window.innerWidth <= 749 && !this.slider) {
            this.init();
          }
        });
      }

      init() {
        this.slider = this.querySelector('.ai-tech-banners__slider-{{ ai_gen_id }}');
        this.dotsContainer = this.querySelector('.ai-tech-banners__dots-{{ ai_gen_id }}');
        this.slides = Array.from(this.slider.querySelectorAll('.ai-tech-banner-{{ ai_gen_id }}'));

        if (this.slides.length === 0) return;

        this.createDots();
        this.setupScrollListener();
      }

      createDots() {
        this.dotsContainer.innerHTML = '';
        this.slides.forEach((slide, index) => {
          const dot = document.createElement('div');
          dot.className = 'ai-tech-banners__dot-{{ ai_gen_id }}';
          if (index === 0) dot.classList.add('active');
          this.dotsContainer.appendChild(dot);
        });
      }

      setupScrollListener() {
        let scrollTimeout;
        this.slider.addEventListener('scroll', () => {
          clearTimeout(scrollTimeout);
          scrollTimeout = setTimeout(() => {
            this.updateActiveDot();
          }, 100);
        });
      }

      updateActiveDot() {
        const scrollLeft = this.slider.scrollLeft;
        const slideWidth = this.slides[0].offsetWidth;
        const gap = parseInt(getComputedStyle(this.slider).gap) || 0;
        const newIndex = Math.round(scrollLeft / (slideWidth + gap));

        if (newIndex !== this.currentIndex) {
          this.currentIndex = newIndex;
          const dots = this.dotsContainer.querySelectorAll('.ai-tech-banners__dot-{{ ai_gen_id }}');
          dots.forEach((dot, index) => {
            if (index === this.currentIndex) {
              dot.classList.add('active');
            } else {
              dot.classList.remove('active');
            }
          });
        }
      }
    }

    customElements.define('tech-banners-slider-{{ ai_gen_id }}', TechBannersSlider{{ ai_gen_id }});
  })();
</script>

