{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{%- assign MENU_HANDLE = block.settings.menu_handle -%}
{%- assign url_collection = collection -%}
{%- assign prod_col_handles = product.collections | map: 'handle' -%}

{%- assign active_collection = nil -%}
{%- assign nav = linklists[MENU_HANDLE] -%}
{%- if nav and nav.links.size > 0 -%}
  {%- assign deep_match = nil -%}
  {%- assign mid_match  = nil -%}
  {%- assign top_match  = nil -%}
  {%- for l1 in nav.links -%}
    {%- if l1.type == 'collection_link' and prod_col_handles contains l1.object.handle -%}
      {%- assign top_match = l1.object -%}
    {%- endif -%}
    {%- if l1.links and l1.links.size > 0 -%}
      {%- for l2 in l1.links -%}
        {%- if l2.type == 'collection_link' and prod_col_handles contains l2.object.handle -%}
          {%- assign mid_match = l2.object -%}
        {%- endif -%}
        {%- if l2.links and l2.links.size > 0 -%}
          {%- for l3 in l2.links -%}
            {%- if l3.type == 'collection_link' and prod_col_handles contains l3.object.handle -%}
              {%- assign deep_match = l3.object -%}{%- break -%}
            {%- endif -%}
          {%- endfor -%}
        {%- endif -%}
        {%- if deep_match -%}{%- break -%}{%- endif -%}
      {%- endfor -%}
    {%- endif -%}
    {%- if deep_match -%}{%- break -%}{%- endif -%}
  {%- endfor -%}
  {%- assign active_collection = deep_match | default: mid_match | default: top_match -%}
{%- endif -%}
{%- if active_collection == nil -%}{%- assign active_collection = url_collection -%}{%- endif -%}
{%- if active_collection == nil -%}{%- assign active_collection = product.collections.first -%}{%- endif -%}

{%- assign first_product = nil -%}
{%- assign last_product  = nil -%}
{%- assign prev_product  = nil -%}
{%- assign next_product  = nil -%}
{%- assign prev_candidate = nil -%}
{%- assign found_current = false -%}

{%- if active_collection and active_collection.products.size > 0 -%}
  {%- assign first_product = active_collection.products.first -%}
  {%- assign last_product = active_collection.products.last -%}
  
  {%- for prod in active_collection.products -%}
    {%- if found_current == false -%}
      {%- if prod.id == product.id -%}
        {%- assign found_current = true -%}
        {%- assign prev_product = prev_candidate -%}
      {%- else -%}
        {%- assign prev_candidate = prod -%}
      {%- endif -%}
    {%- elsif next_product == nil -%}
      {%- assign next_product = prod -%}
      {%- break -%}
    {%- endif -%}
  {%- endfor -%}
{%- endif -%}

{% style %}
  .ai-breadcrumb-nav-{{ ai_gen_id }} {
    width: 100%;
    background-color: rgb(var(--color-background));
    padding: {{ block.settings.padding_top }}px 0 {{ block.settings.padding_bottom }}px;
  }

  .ai-breadcrumb-nav-container-{{ ai_gen_id }} {
    max-width: {{ settings.page_width }}px;
    margin: {{ block.settings.margin_top }}px auto {{ block.settings.margin_bottom }}px;
    padding: 0 {{ block.settings.horizontal_padding }}px;
  }

  .ai-breadcrumb-wrapper-{{ ai_gen_id }} {
    display: flex;
    align-items: center;
    justify-content: space-between;
    gap: 20px;
    flex-wrap: wrap;
  }

  .ai-breadcrumb-list-{{ ai_gen_id }} {
    display: flex;
    align-items: center;
    gap: 8px;
    list-style: none;
    margin: 0;
    padding: 0;
    flex-wrap: wrap;
    font-size: {{ block.settings.breadcrumb_font_size }}px;
    color: rgb(var(--color-foreground));
  }

  .ai-breadcrumb-item-{{ ai_gen_id }} {
    display: flex;
    align-items: center;
    gap: 8px;
  }

  .ai-breadcrumb-link-{{ ai_gen_id }} {
    color: rgba(var(--color-foreground), {{ block.settings.link_opacity | divided_by: 100.0 }});
    text-decoration: none;
    transition: color 0.2s ease;
  }

  .ai-breadcrumb-link-{{ ai_gen_id }}:hover {
    color: rgb(var(--color-foreground));
    text-decoration: underline;
  }

  .ai-breadcrumb-separator-{{ ai_gen_id }} {
    color: rgba(var(--color-foreground), 0.4);
    font-size: 12px;
  }

  .ai-breadcrumb-current-{{ ai_gen_id }} {
    color: rgb(var(--color-foreground));
    font-weight: 500;
  }

  .ai-product-nav-{{ ai_gen_id }} {
    display: flex;
    align-items: center;
    gap: 12px;
  }

  .ai-product-nav-btn-{{ ai_gen_id }} {
    position: relative;
    display: flex;
    align-items: center;
    justify-content: center;
    width: {{ block.settings.nav_button_size }}px;
    height: {{ block.settings.nav_button_size }}px;
    background-color: rgba(var(--color-foreground), {{ block.settings.button_bg_opacity | divided_by: 100.0 }});
    border: {{ block.settings.button_border_width }}px solid rgba(var(--color-foreground), {{ block.settings.button_border_opacity | divided_by: 100.0 }});
    border-radius: {{ block.settings.button_border_radius }}px;
    cursor: pointer;
    transition: all 0.2s ease;
    color: rgb(var(--color-background));
    text-decoration: none;
  }

  .ai-product-nav-btn-{{ ai_gen_id }}:hover {
    background-color: rgb(var(--color-foreground));
    transform: translateY(-2px);
  }

  .ai-product-nav-btn-{{ ai_gen_id }}:disabled {
    opacity: 0.3;
    cursor: not-allowed;
    pointer-events: none;
  }

  .ai-product-nav-btn-{{ ai_gen_id }} svg {
    width: 16px;
    height: 16px;
  }

  .ai-product-nav-popup-{{ ai_gen_id }} {
    position: absolute;
    bottom: calc(100% + 12px);
    left: 50%;
    transform: translateX(-50%);
    width: 210px;
    background-color: rgb(var(--color-background));
    border: {{ settings.popup_border_thickness }}px solid rgba(var(--color-foreground), {{ settings.popup_border_opacity | divided_by: 100.0 }});
    border-radius: {{ settings.popup_corner_radius }}px;
    box-shadow: 0 {{ settings.popup_shadow_vertical_offset }}px {{ settings.popup_shadow_blur }}px rgba(var(--color-shadow), {{ settings.popup_shadow_opacity | divided_by: 100.0 }});
    padding: 0;
    opacity: 0;
    visibility: hidden;
    transition: opacity 0.2s ease, visibility 0.2s ease, transform 0.2s ease;
    z-index: 1000;
    pointer-events: none;
  }

  .ai-product-nav-btn-{{ ai_gen_id }}:hover .ai-product-nav-popup-{{ ai_gen_id }} {
    opacity: 1;
    visibility: visible;
    pointer-events: auto;
  }

  .ai-product-nav-popup-content-{{ ai_gen_id }} {
    display: flex;
    align-items: center;
    gap: 0;
    padding: 0;
    text-decoration: none;
    color: rgb(var(--color-foreground));
  }

  .ai-product-nav-popup-image-{{ ai_gen_id }} {
    width: 50px;
    height: 50px;
    flex-shrink: 0;
    overflow: hidden;
    border-radius: {{ settings.popup_corner_radius }}px 0 0 {{ settings.popup_corner_radius }}px;
  }

  .ai-product-nav-popup-image-{{ ai_gen_id }} img {
    width: 100%;
    height: 100%;
    object-fit: cover;
    display: block;
  }

  .ai-product-nav-popup-info-{{ ai_gen_id }} {
    flex: 1;
    padding: 0 15px 0 23px;
    min-width: 0;
  }

  .ai-product-nav-popup-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.popup_title_size }}px;
    font-weight: 500;
    margin: 0;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    line-height: 1.3;
  }

  .ai-product-nav-popup-sku-{{ ai_gen_id }} {
    font-size: {{ block.settings.popup_sku_size }}px;
    margin: 4px 0 0;
    color: rgba(var(--color-foreground), 0.6);
    font-family: monospace;
  }

  .ai-product-nav-popup-price-{{ ai_gen_id }} {
    font-size: {{ block.settings.popup_price_size }}px;
    margin: 4px 0 0;
    color: rgba(var(--color-foreground), 0.7);
  }

  @media screen and (max-width: 749px) {
    .ai-breadcrumb-wrapper-{{ ai_gen_id }} {
      flex-direction: column;
      align-items: flex-start;
      gap: 12px;
    }

    .ai-product-nav-{{ ai_gen_id }} {
      width: 100%;
      justify-content: flex-end;
    }

    .ai-breadcrumb-list-{{ ai_gen_id }} {
      font-size: {{ block.settings.breadcrumb_font_size | times: 0.9 }}px;
    }

    .ai-product-nav-popup-{{ ai_gen_id }} {
      width: 180px;
    }
  }
{% endstyle %}

<breadcrumb-navigation-{{ ai_gen_id }} class="ai-breadcrumb-nav-{{ ai_gen_id }} color-{{ block.settings.color_scheme }}" {{ block.shopify_attributes }}>
  <div class="ai-breadcrumb-nav-container-{{ ai_gen_id }}">
    <div class="ai-breadcrumb-wrapper-{{ ai_gen_id }}">
      <nav aria-label="Breadcrumb">
        <ol class="ai-breadcrumb-list-{{ ai_gen_id }}">
          <li class="ai-breadcrumb-item-{{ ai_gen_id }}">
            <a href="{{ routes.root_url }}" class="ai-breadcrumb-link-{{ ai_gen_id }}">
              {{ block.settings.home_text }}
            </a>
            <span class="ai-breadcrumb-separator-{{ ai_gen_id }}">{{ block.settings.separator }}</span>
          </li>
          
          {%- if active_collection -%}
            <li class="ai-breadcrumb-item-{{ ai_gen_id }}">
              <a href="{{ active_collection.url }}" class="ai-breadcrumb-link-{{ ai_gen_id }}">
                {{ active_collection.title }}
              </a>
              <span class="ai-breadcrumb-separator-{{ ai_gen_id }}">{{ block.settings.separator }}</span>
            </li>
          {%- endif -%}
          
          <li class="ai-breadcrumb-item-{{ ai_gen_id }}">
            <span class="ai-breadcrumb-current-{{ ai_gen_id }}">{{ product.title }}</span>
          </li>
        </ol>
      </nav>

      {%- if block.settings.show_product_navigation and active_collection -%}
        <div class="ai-product-nav-{{ ai_gen_id }}">
          {%- if prev_product -%}
            <a 
              href="{{ prev_product.url }}" 
              class="ai-product-nav-btn-{{ ai_gen_id }}"
              aria-label="Previous product"
            >
              <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 19l-7-7 7-7" />
              </svg>
              <div class="ai-product-nav-popup-{{ ai_gen_id }}">
                <div class="ai-product-nav-popup-content-{{ ai_gen_id }}">
                  <div class="ai-product-nav-popup-image-{{ ai_gen_id }}">
                    {%- if prev_product.featured_image -%}
                      <img 
                        src="{{ prev_product.featured_image | image_url: width: 100 }}" 
                        alt="{{ prev_product.featured_image.alt | escape }}"
                        loading="lazy"
                        width="100"
                        height="100"
                      >
                    {%- endif -%}
                  </div>
                  <div class="ai-product-nav-popup-info-{{ ai_gen_id }}">
                    <h3 class="ai-product-nav-popup-title-{{ ai_gen_id }}">{{ prev_product.title }}</h3>
                    {%- if block.settings.show_sku and prev_product.selected_or_first_available_variant.sku != blank -%}
                      <div class="ai-product-nav-popup-sku-{{ ai_gen_id }}">SKU: {{ prev_product.selected_or_first_available_variant.sku }}</div>
                    {%- endif -%}
                    {%- if block.settings.show_price -%}
                      <div class="ai-product-nav-popup-price-{{ ai_gen_id }}">{{ prev_product.price | money }}</div>
                    {%- endif -%}
                  </div>
                </div>
              </div>
            </a>
          {%- else -%}
            <button class="ai-product-nav-btn-{{ ai_gen_id }}" disabled aria-label="No previous product">
              <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 19l-7-7 7-7" />
              </svg>
            </button>
          {%- endif -%}

          {%- if next_product -%}
            <a 
              href="{{ next_product.url }}" 
              class="ai-product-nav-btn-{{ ai_gen_id }}"
              aria-label="Next product"
            >
              <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7" />
              </svg>
              <div class="ai-product-nav-popup-{{ ai_gen_id }}">
                <div class="ai-product-nav-popup-content-{{ ai_gen_id }}">
                  <div class="ai-product-nav-popup-image-{{ ai_gen_id }}">
                    {%- if next_product.featured_image -%}
                      <img 
                        src="{{ next_product.featured_image | image_url: width: 100 }}" 
                        alt="{{ next_product.featured_image.alt | escape }}"
                        loading="lazy"
                        width="100"
                        height="100"
                      >
                    {%- endif -%}
                  </div>
                  <div class="ai-product-nav-popup-info-{{ ai_gen_id }}">
                    <h3 class="ai-product-nav-popup-title-{{ ai_gen_id }}">{{ next_product.title }}</h3>
                    {%- if block.settings.show_sku and next_product.selected_or_first_available_variant.sku != blank -%}
                      <div class="ai-product-nav-popup-sku-{{ ai_gen_id }}">SKU: {{ next_product.selected_or_first_available_variant.sku }}</div>
                    {%- endif -%}
                    {%- if block.settings.show_price -%}
                      <div class="ai-product-nav-popup-price-{{ ai_gen_id }}">{{ next_product.price | money }}</div>
                    {%- endif -%}
                  </div>
                </div>
              </div>
            </a>
          {%- else -%}
            <button class="ai-product-nav-btn-{{ ai_gen_id }}" disabled aria-label="No next product">
              <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7" />
              </svg>
            </button>
          {%- endif -%}
        </div>
      {%- endif -%}
    </div>
  </div>
</breadcrumb-navigation-{{ ai_gen_id }}>

<script>
  (function() {
    class BreadcrumbNavigation{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
      }

      connectedCallback() {
        this.setupAccessibility();
      }

      setupAccessibility() {
        const popups = this.querySelectorAll('.ai-product-nav-popup-{{ ai_gen_id }}');
        const buttons = this.querySelectorAll('.ai-product-nav-btn-{{ ai_gen_id }}');
        
        buttons.forEach((button, index) => {
          const popup = popups[index];
          if (!popup) return;
          
          button.addEventListener('mouseenter', function() {
            button.setAttribute('aria-expanded', 'true');
          });
          
          button.addEventListener('mouseleave', function() {
            button.setAttribute('aria-expanded', 'false');
          });
        });
      }
    }

    customElements.define('breadcrumb-navigation-{{ ai_gen_id }}', BreadcrumbNavigation{{ ai_gen_id }});
  })();
</script>

