    "faqs_W7dAza": {
      "type": "faqs",
      "blocks": {
        "qa_YDLtyV": {
          "type": "QA",
          "settings": {
            "question": "How do I ensure my PPE is compliant with NFPA 70E and OSHA 1910 Subpart S?",
            "answer": "All our arc-rated gear is tested to ASTM F1506 and labeled with Arc Thermal Performance Value (ATPV) and CAT level. We also provide documentation, user guides, and audit-ready certification packets."
          }
        },
        "qa_y4HaQ7": {
          "type": "QA",
          "settings": {
            "question": "Do you offer winter-ready PPE for linemen in northern states like Minnesota or Michigan?",
            "answer": "Yes. We supply FR-insulated parkas, bibs, balaclavas, and gloves that maintain arc ratings while keeping crews warm and mobile in freezing conditions."
          }
        },
        "qa_gx38ej": {
          "type": "QA",
          "settings": {
            "question": "Can your dielectric gear handle wet weather or emergency storm response?",
            "answer": "Absolutely. Our Class 0–4 gloves, EH-rated boots, and insulated tools are designed to retain integrity in moisture, mud, and flood-prone zones."
          }
        },
        "qa_VDRrYt": {
          "type": "QA",
          "settings": {
            "question": "Do you offer gear that meets Category 3 or 4 arc flash protection levels?",
            "answer": "Yes. Our full kits include multi-layer CAT 3 & CAT 4 solutions, with arc hoods, long coats, gloves, face shields, and insulated footwear."
          }
        },
        "qa_CE66Ty": {
          "type": "QA",
          "settings": {
            "question": "How fast can I equip a temporary or traveling crew for emergency grid repairs?",
            "answer": "We maintain rapid-response inventory for utility companies and can deploy pre-kitted arc flash PPE within 24–48 hours nationwide."
          }
        },
        "qa_KYNNYR": {
          "type": "QA",
          "settings": {
            "question": "Can I custom brand or color-code PPE for specific utility divisions or teams?",
            "answer": "Yes. We offer logo embroidery, department patches, and color-coded options (e.g., green for maintenance, red for emergency) without affecting compliance."
          }
        },
        "qa_i6DWyE": {
          "type": "QA",
          "settings": {
            "question": "What safety training do you provide with arc flash kits?",
            "answer": "We include printable posters, training videos, and donning/doffing charts—all aligned with NFPA 70E best practices. Custom onboarding is available."
          }
        },
        "qa_Ai4nT7": {
          "type": "QA",
          "settings": {
            "question": "Do you have PPE suitable for substations and overhead work?",
            "answer": "Yes. We provide pole-top kits, FR climbing harnesses, and high-visibility arc-rated gear ideal for substations, transmission towers, and aerial crews."
          }
        },
        "qa_8mkF7U": {
          "type": "QA",
          "settings": {
            "question": "How often should electrical PPE be replaced or inspected?",
            "answer": "We recommend daily pre-use inspections and scheduled replacement per OSHA/ASTM timelines. We provide checklists and inspection tags with all kits."
          }
        },
        "qa_mDPhzF": {
          "type": "QA",
          "settings": {
            "question": "Can you help centralize PPE procurement for multiple utility districts?",
            "answer": "Definitely. We support centralized fulfillment, site-specific PPE planning, and digital reordering portals for regional or national utility organizations."
          }
        }
      },
