    "faqs_9eaLgA": {
      "type": "faqs",
      "blocks": {
        "qa_K4HeNc": {
          "type": "QA",
          "settings": {
            "question": "Why Logo?",
            "answer": "Your logo and messaging is important to your business and Defender Safety wants to help foster your brand identity along with your messaging on our safety products.  By doing so it communicates ownership, team, quality, and instantly brings your company’s values and culture to your employees. Let Caco American help you with the details!"
          }
        },
        "qa_WCFAVm": {
          "type": "QA",
          "settings": {
            "question": "What is the cost to logo product?",
            "answer": "Our pricing is quoted per job by our Customer Support Team.  We will require your artwork and any messaging you would like to put on the products.  Once we have this information our team can quote."
          }
        },
        "qa_gwteJJ": {
          "type": "QA",
          "settings": {
            "question": "What are the fees associated with custom printing?",
            "answer": "There is a $50 plate set-up fee as a one-time charge for creating the printing plate used in the printing process. The additional fee for printing each helmet would vary depending on the quantity of helmets ordered. Our Customer Support Team can send you an estimate once we have your logo and helmet quantity."
          }
        },
        "qa_M6tXjA": {
          "type": "QA",
          "settings": {
            "question": "How long does it take to get my custom printed products?",
            "answer": "Our logo process for the initial order typically takes two (2) weeks from the time you approve the artwork, send an official PO, and the order is paid in full.  Repeat orders typically take less time."
          }
        },
        "qa_GBibgM": {
          "type": "QA",
          "settings": {
            "question": "What if I need to have my order faster?",
            "answer": "We will try to support your request and we will be honest with you on timelines.  If we can’t meet your timeline we will let you know up front so there are no surprises."
          }
        },
        "qa_mrCqCw": {
          "type": "QA",
          "settings": {
            "question": "What do you need for artwork?",
            "answer": "We need a high resolution (3000 DPI or greater) in an AI or EPS  file of your logo."
          }
        },
        "qa_eXh9j8": {
          "type": "QA",
          "settings": {
            "question": "Can you accommodate multi-color logos?",
            "answer": "Yes, we can accept logos with up to 4 colors."
          }
        },
        "qa_qQfaiK": {
          "type": "QA",
          "settings": {
            "question": "Once I approve can I get a sample?",
            "answer": "We cannot supply samples of a finished logoed product. You will receive a digital mock-up of what the finished product will look like along with the details for your approval."
          }
        },
        "qa_DqXHWJ": {
          "type": "QA",
          "settings": {
            "question": "Can I put any message or logo on products?",
            "answer": "As part of the our code of conduct we reserve the right to decline any design submission we deem vulgar, offensive, uses profanity or infringe on any intellectual property or third party rights."
          }
        },
        "qa_tAyXFN": {
          "type": "QA",
          "settings": {
            "question": "Is there minimum order quantity?",
            "answer": "There is a 12 piece minimum order."
          }
        }
      },
