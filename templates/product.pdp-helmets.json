           data-preview-img=\"{% if next_img %}{{ next_img | image_url: width: 220 }}{% endif %}\">\n                <svg viewBox=\"0 0 24 24\" aria-hidden=\"true\" focusable=\"false\"><path d=\"M9 6l6 6-6 6\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\"/></svg>\n                <div class=\"mh-preview\" role=\"dialog\" aria-label=\"Next product preview\" aria-hidden=\"true\">\n                  <div class=\"row\">\n                    <div class=\"thumb\"><img alt=\"\" src=\"\"></div>\n                    <div class=\"title\"></div>\n                    <div class=\"sku\"></div>\n                  </div>\n                </div>\n              </a>\n            </div>\n          {% endif %}\n        </div>\n      </div>\n    </div>\n  </div>\n</div>\n\n<script>\n/* REPLACEMENT SCRIPT: move preview to body to avoid clipping; Esc-to-close; fix link navigation (no preventDefault on click) */\n(function(){\n  const clamp = (v, a, b) => Math.max(a, Math.min(v, b));\n\n  document.querySelectorAll('.mh-iconbtn').forEach(function(btn){\n    const card = btn.querySelector('.mh-preview');\n    if (!card) return;\n\n    const imgEl = card.querySelector('.thumb img');\n    const titleEl = card.querySelector('.title');\n    const skuEl = card.querySelector('.sku');\n\n    // Keep references to restore DOM position later\n    let originalParent = null;\n    let originalNext = null;\n    let isOpen = false;\n\n    function fill() {\n      const src = btn.getAttribute('data-preview-img') || '';\n      const t = btn.getAttribute('data-preview-title') || '';\n      const s = btn.getAttribute('data-preview-sku') || '';\n      if (src) imgEl.src = src;\n      titleEl.textContent = t;\n      skuEl.textContent = s ? ('SKU: ' + s) : 'SKU: —';\n    }\n\n    function measureAndPlace() {\n      const r = btn.getBoundingClientRect();\n      const vw = window.innerWidth, vh = window.innerHeight;\n      const popupW = 210, GAP = 4;\